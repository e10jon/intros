{
  "expo": {
    "name": "intros",
    "slug": "intros",
    "scheme": "intros",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "bundleIdentifier": "org.intros.social",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
